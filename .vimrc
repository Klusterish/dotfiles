set number
set smartindent
set runtimepath^=~/.vim/bundle/ctrlp.vim
set runtimepath^=~/.vim/bundle/ag
set runtimepath^=~/.vim/bundle/vim-coffee-script
set expandtab
set tabstop=2
set shiftwidth=2
set list
set listchars=tab:▸\ ,eol:¬
syntax on
color idlefingers
nmap <C-k> :tabn<cr>
nmap <C-j> :tabp<cr>

